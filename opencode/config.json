  "mcp": {
    "r2copilot": {
      "type": "local",
      "command": [ "/Users/pancake/prg/r2-copilot/start.sh" ],
      "enabled": false
    },
    "r2mcp": {
      "type": "local",
      "command": [
        "r2pm", "-r", "r2mcp", "-m"
